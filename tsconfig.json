{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    },
    "outDir": "dist",
              
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,               
    "skipLibCheck": true
  }
}